    "eslint-plugin-unicorn": "^21.0.0",
    "eslint-plugin-vue": "^7.0.0-beta.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.3",
    "rollup-plugin-dts": "^1.4.10",
